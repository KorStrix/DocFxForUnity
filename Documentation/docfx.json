{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [ "*.csproj" ]
        }
      ],
      "filter": "filterConfig.yml",
      "dest": "api/"
    }
  ],
  "build": {
    "globalMetadata":
    {
      "_appTitle": "Example Unity documentation",
      "_appFooter": "Example Unity documentation",
      "_enableSearch": true
    },
    "content": [
      {
        "files": [ "api/*.yml" ]
      },
      {
        "src": "manual/",
        "dest": "manual/",
        "files": [ "toc.yml", "*.md" ]
      },
      {
        "files": [ "toc.yml", "index.md" ]
      }
    ],
    "resource": [
      {
        "files": [ "images/*" ]
      }
    ],
    "xref": [ "https://normanderwan.github.io/DocFxForUnity/Unity/xrefmap.yml" ],
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "dest": "../_site/"
  }
}